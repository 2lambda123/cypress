{
  "extends": "../ts/tsconfig.json",
  "include": ["src"],
  "compilerOptions": {
    "lib": ["esnext", "DOM"],
    "strict": true,
    "allowJs": false,
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "noUnusedLocals": false,
    "noUncheckedIndexedAccess": true,
    "types": ["cypress"],
  }
}
