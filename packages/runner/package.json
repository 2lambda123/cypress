{
  "name": "@packages/runner",
  "version": "0.0.0-development",
  "private": true,
  "browser": "src/index.js",
  "scripts": {
    "build": "ts-node ../../scripts/run-webpack",
    "build-prod": "cross-env NODE_ENV=production yarn build",
    "clean-deps": "rimraf node_modules",
    "cypress:open": "echo \"These tests have been moved to @packages/app. \" && exit 1",
    "cypress:run": "echo \"These tests have been moved to @packages/app. \" && exit 1",
    "postinstall": "echo '@packages/runner needs: yarn build'",
    "test": "yarn test-unit test-types",
    "test-types": "ts-node ../../scripts/cypress.js run --component --project .",
    "test-unit": "mocha --config test/.mocharc.json src/*.spec.* src/**/*.spec.* --exit",
    "watch": "ts-node ../../scripts/run-webpack --watch --progress"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
